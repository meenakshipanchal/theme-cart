<div class="edd-checker-container">
    <h3>{{- 'location.svg' | inline_asset_content -}} Delivery Date</h3>
    
    <div class="pincode-input-section">
        <label for="delivery-pincode">Enter Pin code to check <strong>delivery date</strong> for <span id="product-title-display">Product</span>:</label>
        <div class="input-group">
            <input 
                type="text" 
                id="delivery-pincode" 
                placeholder="Enter 6-digit pincode (e.g., 121008)"
                maxlength="6"
                pattern="[0-9]{6}"
            >
            <button id="check-edd-btn">Check</button>
        </div>
    </div>
    
    <input type="hidden" id="product-id" value="{{ product.id }}">
    <input type="hidden" id="product-title" value="{{ product.title | escape }}">
    <input type="hidden" id="product-weight" value="{% if product.metafields.custom.weight %}{{ product.metafields.custom.weight | divided_by: 1000.0 }}{% else %}0.5{% endif %}">
    <input type="hidden" id="product-price" value="{{ product.price | divided_by: 100.0 }}">
    <input type="hidden" id="product-sku" value="{{ product.variants.first.sku | default: 'UNKNOWN_SKU' }}">
    <input type="hidden" id="product-vendor" value="{{ product.vendor | escape }}">
    <input type="hidden" id="product-type" value="{{ product.type | escape }}">
    
    <!-- <button class="toggle-debug" onclick="toggleDebug()">Show Debug Logs</button> -->
    
    <div id="debug-console" style="display: none;">
        <div style="display: flex; justify-content: space-between; align-items: center; margin-bottom: 10px;">
            <strong>üîç Debug Log:</strong>
            <button onclick="clearDebugLogs()" style="padding: 2px 8px; font-size: 10px; background: #dc3545; color: white; border: none; border-radius: 3px; cursor: pointer;">
                Clear Logs
            </button>
        </div>
        <div id="debug-logs"></div>
    </div>
    
    <div id="loading-state" style="display: none;">
        <div class="loading-spinner"></div>
        <p style="margin: 0;">Checking delivery options...</p>
    </div>
    
    <div id="edd-results" style="display: none;">
        <div id="delivery-info"></div>
    </div>
    
    <div id="edd-error" style="display: none;">
        <h4>‚ùå Error</h4>
        <p id="error-message"></p>
    </div>
</div>

<script>
    const warehouses = {
        'Anveshan Farm Technologies Manesar': {
            pincode: '122050',
            region: '122',
            state: 'Haryana'
        },
        'Emiza Bangalore': {
            pincode: '562123',
            region: '562',
            state: 'Karnataka'
        },
        'Emiza Bhiwandi': {
            pincode: '421302',
            region: '421',
            state: 'Maharashtra'
        },
        'Emiza Kolkata': {
            pincode: '711302',
            region: '711',
            state: 'West Bengal'
        },
        'Prozo-GGN': {
            pincode: '122413',
            region: '122',
            state: 'Haryana'
        }
    };

    const SHIPROCKET_CONFIG = {
        email: 'kunalprajapati@anveshan.farm',
        password: '2Yy6lL%VqQklFh3@'
    };

    let authToken = null;
    let tokenExpiry = null;

    function debugLog(message, data = null, type = 'info') {
        const timestamp = new Date().toLocaleTimeString();
        const logMessage = `${timestamp} ${message}`;
        
        console.log(logMessage, data || '');
        
        const debugLogs = document.getElementById('debug-logs');
        if (debugLogs) {
            const logEntry = document.createElement('div');
            logEntry.className = `log-entry ${type === 'error' ? 'log-error' : type === 'success' ? 'log-success' : type === 'warning' ? 'log-warning' : ''}`;
            
            let logContent = `<span class="log-timestamp">${timestamp}</span> ${message}`;
            
            if (data) {
                logContent += `\nüìÑ Data: ${JSON.stringify(data, null, 2)}`;
            }
            
            logEntry.innerHTML = `<pre style="margin: 0; white-space: pre-wrap; word-break: break-word;">${logContent}</pre>`;
            debugLogs.appendChild(logEntry);
            debugLogs.scrollTop = debugLogs.scrollHeight;
        }
    }

    function toggleDebug() {
        const debugConsole = document.getElementById('debug-console');
        const toggleBtn = event.target;
        
        if (debugConsole.style.display === 'none') {
            debugConsole.style.display = 'block';
            toggleBtn.textContent = 'Hide Debug Logs';
        } else {
            debugConsole.style.display = 'none';
            toggleBtn.textContent = 'Show Debug Logs';
        }
    }

    function clearDebugLogs() {
        const debugLogs = document.getElementById('debug-logs');
        if (debugLogs) {
            debugLogs.innerHTML = '';
        }
    }

    function validatePincode(pincode) {
        const pincodeRegex = /^[1-9][0-9]{5}$/;
        return pincodeRegex.test(pincode);
    }

    async function validatePincodeAPI(pincode) {
        debugLog('üîç Validating pincode with API...', { pincode });
        
        try {
            const response = await fetch(`https://api.postalpincode.in/pincode/${pincode}`);
            const data = await response.json();
            
            debugLog('üìç Pincode validation response:', data);
            
            if (data && data[0] && data[0].Status === "Success") {
                const result = {
                    valid: true,
                    district: data[0].PostOffice[0].District,
                    state: data[0].PostOffice[0].State,
                    area: data[0].PostOffice[0].Name,
                    division: data[0].PostOffice[0].Division,
                    region: data[0].PostOffice[0].Region
                };
                debugLog('‚úÖ Pincode validation successful:', result, 'success');
                return result;
            }
            
            debugLog('‚ùå Pincode validation failed - Invalid pincode', null, 'error');
            return { valid: false };
        } catch (error) {
            debugLog('‚ö†Ô∏è Pincode validation API error, using basic validation:', error.message, 'warning');
            return { valid: validatePincode(pincode) };
        }
    }

    function findNearestWarehouse(userPincode) {
        debugLog('üè™ Finding nearest warehouse...', { userPincode });
        
        const userRegion = userPincode.substring(0, 3);
        let nearestWarehouse = null;
        let minDistance = Infinity;
        
        for (const [warehouseName, warehouseData] of Object.entries(warehouses)) {
            if (warehouseData.region === userRegion) {
                nearestWarehouse = {
                    name: warehouseName,
                    ...warehouseData
                };
                debugLog('üéØ Exact region match found:', nearestWarehouse, 'success');
                return nearestWarehouse;
            }
        }
        
        for (const [warehouseName, warehouseData] of Object.entries(warehouses)) {
            const distance = Math.abs(parseInt(userRegion) - parseInt(warehouseData.region));
            
            if (distance < minDistance) {
                minDistance = distance;
                nearestWarehouse = {
                    name: warehouseName,
                    ...warehouseData,
                    distance: distance
                };
            }
        }
        
        debugLog('üìç Nearest warehouse found:', nearestWarehouse, 'success');
        return nearestWarehouse;
    }

    function getCurrentProductDetails() {
        const productData = {
            id: document.getElementById('product-id').value,
            weight: parseFloat(document.getElementById('product-weight').value) || 0.5,
            price: parseFloat(document.getElementById('product-price').value) || 500,
            title: document.getElementById('product-title').value || 'Unknown Product',
            sku: document.getElementById('product-sku').value || 'UNKNOWN_SKU',
            vendor: document.getElementById('product-vendor').value || 'Anveshan',
            product_type: document.getElementById('product-type').value || 'Unknown Type'
        };
        
        debugLog('üì¶ Current product details:', productData);
        return productData;
    }

    async function getShiprocketToken() {
        if (authToken && tokenExpiry && Date.now() < tokenExpiry) {
            debugLog('üîë Using cached authentication token');
            return authToken;
        }
        
        debugLog('üîê Requesting new authentication token...', {
            email: SHIPROCKET_CONFIG.email,
            password: '***HIDDEN***'
        });
        
        try {
            const response = await fetch('https://apiv2.shiprocket.in/v1/external/auth/login', {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({
                    email: SHIPROCKET_CONFIG.email,
                    password: SHIPROCKET_CONFIG.password
                })
            });
            
            const data = await response.json();
            
            debugLog('üîê Authentication response:', {
                status: response.status,
                ok: response.ok,
                data: data
            });
            
            if (!response.ok) {
                debugLog('‚ùå Authentication failed:', {
                    status: response.status,
                    message: data.message || 'Unknown error'
                }, 'error');
                throw new Error(`Authentication failed: ${data.message || 'Invalid credentials'}`);
            }
            
            if (data.token) {
                authToken = data.token;
                tokenExpiry = Date.now() + (23 * 60 * 60 * 1000);
                
                debugLog('‚úÖ Authentication successful!', {
                    token: `${authToken.substring(0, 20)}...`,
                    expires: new Date(tokenExpiry).toLocaleString()
                }, 'success');
                
                return authToken;
            }
            
            throw new Error('No token received from authentication');
        } catch (error) {
            debugLog('‚ùå Error getting Shiprocket token:', error.message, 'error');
            throw error;
        }
    }

    async function checkServiceability(pickupPincode, deliveryPincode, weight, price) {
        debugLog('üöö Checking serviceability with Shiprocket API...', {
            pickupPincode,
            deliveryPincode,
            weight,
            price
        });
        
        const token = await getShiprocketToken();
        
        const params = new URLSearchParams({
            pickup_postcode: pickupPincode,
            delivery_postcode: deliveryPincode,
            weight: weight,
            cod: 0,
            declared_value: price
        });
        
        const url = `https://apiv2.shiprocket.in/v1/external/courier/serviceability/?${params.toString()}`;
        
        debugLog('üì§ Serviceability request URL:', { url, params: Object.fromEntries(params) });
        
        try {
            const response = await fetch(url, {
                method: 'GET',
                headers: {
                    'Content-Type': 'application/json',
                    'Authorization': `Bearer ${token}`
                }
            });
            
            debugLog('üì• Serviceability response status:', {
                status: response.status,
                ok: response.ok,
                statusText: response.statusText,
                headers: Object.fromEntries(response.headers.entries())
            });
            
            const data = await response.json();
            debugLog('üì¶ Serviceability response data:', data);
            
            if (!response.ok) {
                debugLog('‚ùå Shiprocket API error:', {
                    status: response.status,
                    error: data
                }, 'error');
                throw new Error(`Shiprocket API error: ${response.status} - ${data.message || response.statusText}`);
            }
            
            debugLog('‚úÖ Serviceability data received successfully!', data, 'success');
            return data;
            
        } catch (error) {
            debugLog('‚ùå Error checking serviceability:', error.message, 'error');
            throw error;
        }
    }

    function calculateDeliveryTimeRange(couriers) {
        debugLog('üìä Calculating delivery time range...', { courierCount: couriers.length });
        
        if (!couriers || couriers.length === 0) {
            debugLog('‚ùå No couriers data available', null, 'error');
            return null;
        }
        
        let minDays = Infinity;
        let maxDays = -Infinity;
        
        couriers.forEach((courier, index) => {
            const deliveryDays = parseInt(courier.estimated_delivery_days || courier.etd_days || courier.etd || 999);
            
            debugLog(`üì¶ Courier ${index + 1}: ${courier.courier_name || 'Unknown'} - ${deliveryDays} days`);
            
            if (deliveryDays !== 999 && !isNaN(deliveryDays)) {
                minDays = Math.min(minDays, deliveryDays);
                maxDays = Math.max(maxDays, deliveryDays);
            }
        });
        
        if (minDays === Infinity || maxDays === -Infinity) {
            debugLog('‚ö†Ô∏è No valid delivery days found', null, 'warning');
            return null;
        }
        
        const range = {
            minDays: minDays,
            maxDays: maxDays,
            rangeText: minDays === maxDays ? `${minDays} day${minDays !== 1 ? 's' : ''}` : `${minDays}-${maxDays} days`
        };
        
        debugLog('‚úÖ Delivery time range calculated:', range, 'success');
        return range;
    }

    async function checkDeliveryTime() {
        const userPincode = document.getElementById('delivery-pincode').value.trim();
        const checkButton = document.getElementById('check-edd-btn');
        
        debugLog('üöÄ Starting delivery time check...', { userPincode });
        
        checkButton.disabled = true;
        checkButton.textContent = 'Checking...';
        hideAllSections();
        
        if (!validatePincode(userPincode)) {
            debugLog('‚ùå Invalid pincode format', null, 'error');
            showError('Please enter a valid 6-digit pincode');
            resetButtonState();
            return;
        }
        
        document.getElementById('loading-state').style.display = 'block';
        
        try {
            debugLog('üìç Step 1: Validating pincode...');
            const pincodeValidation = await validatePincodeAPI(userPincode);
            if (!pincodeValidation.valid) {
                debugLog('‚ùå Pincode validation failed', null, 'error');
                showError('This pincode does not exist. Please enter a valid pincode.');
                return;
            }
            
            debugLog('üè™ Step 2: Finding nearest warehouse...');
            const nearestWarehouse = findNearestWarehouse(userPincode);
            
            if (!nearestWarehouse) {
                debugLog('‚ùå No warehouse found', null, 'error');
                showError('Delivery not available to this location');
                return;
            }
            
            debugLog('üì¶ Step 3: Getting product details...');
            const currentProduct = getCurrentProductDetails();
            
            debugLog('üöö Step 4: Checking serviceability with Shiprocket...');
            const serviceabilityData = await checkServiceability(
                nearestWarehouse.pincode,
                userPincode,
                currentProduct.weight,
                currentProduct.price
            );
            
            debugLog('üìã Step 5: Displaying results...');
            displayDeliveryResults(serviceabilityData, nearestWarehouse, userPincode, pincodeValidation);
            
            debugLog('‚úÖ Delivery check completed successfully!', null, 'success');
            
        } catch (error) {
            debugLog('‚ùå Error in delivery check:', error.message, 'error');
            console.error('Error checking delivery time:', error);
            showError(`Unable to check delivery options: ${error.message}`);
        } finally {
            document.getElementById('loading-state').style.display = 'none';
            resetButtonState();
        }
    }

    function displayDeliveryResults(apiResponse, warehouse, userPincode, pincodeInfo) {
        debugLog('üìã Displaying delivery results...', { apiResponse, warehouse, userPincode, pincodeInfo });
        
        const resultsDiv = document.getElementById('delivery-info');
        
        if (!apiResponse) {
            debugLog('‚ùå No API response received', null, 'error');
            showError('No delivery information received from API');
            return;
        }
        
        let couriers = [];
        
        if (apiResponse.data && apiResponse.data.available_courier_companies) {
            couriers = apiResponse.data.available_courier_companies;
        } else if (apiResponse.available_courier_companies) {
            couriers = apiResponse.available_courier_companies;
        } else if (Array.isArray(apiResponse)) {
            couriers = apiResponse;
        } else if (apiResponse.data && Array.isArray(apiResponse.data)) {
            couriers = apiResponse.data;
        }
        
        debugLog('üì¶ Processing couriers data:', { courierCount: couriers.length, couriers });
        
        if (couriers.length === 0) {
            debugLog('‚ùå No couriers available', null, 'error');
            showError('No delivery options available for this pincode');
            return;
