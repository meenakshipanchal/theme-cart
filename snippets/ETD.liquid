        function findNearestWarehouse(userPincode) {
            debugLog('üè™ Finding nearest warehouse...', { userPincode });
            
            const userRegion = userPincode.substring(0, 3);
            let nearestWarehouse = null;
            let minDistance = Infinity;
            
            for (const [warehouseName, warehouseData] of Object.entries(warehouses)) {
                if (warehouseData.region === userRegion) {
                    nearestWarehouse = {
                        name: warehouseName,
                        ...warehouseData
                    };
                    debugLog('üéØ Exact region match found:', nearestWarehouse, 'success');
                    return nearestWarehouse;
                }
            }
            
            for (const [warehouseName, warehouseData] of Object.entries(warehouses)) {
                const distance = Math.abs(parseInt(userRegion) - parseInt(warehouseData.region));
                
                if (distance < minDistance) {
                    minDistance = distance;
                    nearestWarehouse = {
                        name: warehouseName,
                        ...warehouseData,
                        distance: distance
                    };
                }
            }
            
            debugLog('üìç Nearest warehouse found:', nearestWarehouse, 'success');
            return nearestWarehouse;
        }

