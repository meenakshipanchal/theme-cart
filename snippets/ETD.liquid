        function validatePincode(pincode) {
            const pincodeRegex = /^[1-9][0-9]{5}$/;
            return pincodeRegex.test(pincode);
        }

        async function validatePincodeAPI(pincode) {
            debugLog('üîç Validating pincode with API...', { pincode });
            
            try {
                const response = await fetch(`https://api.postalpincode.in/pincode/${pincode}`);
                const data = await response.json();
                
                debugLog('üìç Pincode validation response:', data);
                
                if (data && data[0] && data[0].Status === "Success") {
                    const result = {
                        valid: true,
                        district: data[0].PostOffice[0].District,
                        state: data[0].PostOffice[0].State,
                        area: data[0].PostOffice[0].Name,
                        division: data[0].PostOffice[0].Division,
                        region: data[0].PostOffice[0].Region
                    };
                    debugLog('‚úÖ Pincode validation successful:', result, 'success');
                    return result;
                }
                
                debugLog('‚ùå Pincode validation failed - Invalid pincode', null, 'error');
                return { valid: false };
            } catch (error) {
                debugLog('‚ö†Ô∏è Pincode validation API error, using basic validation:', error.message, 'warning');
                return { valid: validatePincode(pincode) };
            }
        }

